{
    "name": "cloudflare-badges-app",
    "description": "Add badges to your website.",
    "private": true,
    "version": "0.0.0",
    "main": "index.js",
    "scripts": {
        "start": "yarn dev:setup && yarn dev:webpack",
        "dev:webpack": "webpack --watch --colors",
